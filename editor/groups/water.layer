LAYER {
    1-16 {
        NAME: 'coastline'
        TYPE: LINE
        @layerconfig
        DATA {
            1-5: '110m_physical/ne_110m_coastline'
			6-10: '50m_physical/ne_50m_coastline'
            11-16: '10m_physical/ne_10m_coastline'
		}
        CLASS {
            STYLE {
                COLOR: @watercolor_ol
            }
        }
    }
}

LAYER {
    3-16 {
        NAME: 'lakes'
        TYPE: POLYGON
        @layerconfig
        DATA {
            1-4: '110m_physical/ne_110m_lakes'
            5-10: '50m_physical/ne_50m_lakes'
            11-16: '10m_physical/ne_10m_lakes'		
		}
        CLASS {
            STYLE {
                COLOR: @watercolor
                OUTLINECOLOR: @watercolor_ol
            }
        }
    }
}

LAYER {
    5-16 {
        NAME: 'rivers'
        TYPE: LINE
        @layerconfig
        DATA {
            5-10: '50m_physical/ne_50m_rivers_lake_centerlines_scale_rank'
            11-16: '10m_physical/ne_10m_rivers_lake_centerlines_scale_rank'	
		}
    	LABELITEM: "name"
        CLASS {
            EXPRESSION: ([strokeweig] >= 0 AND [strokeweig] <= 0.2)
            STYLE {
                COLOR: @rivercolor  
                WIDTH {
					5-6: 1.5
					7-16: 3
				}
            }
            LABEL {
                6-16 {
                	COLOR: @watercolor_lbl
                	OUTLINECOLOR: 255 255 255
                	FONT: 'scb'
                	TYPE: truetype
                	SIZE: 10
                    REPEATDISTANCE: 100
                    MINDISTANCE: 300
                	ANGLE: FOLLOW
                	POSITION: AUTO
                	PARTIALS: FALSE
                }
            }    
        }
    	CLASS {
            EXPRESSION: ([strokeweig] >= 0.200000001 AND [strokeweig] <= 0.35)
            STYLE {
                COLOR: @rivercolor
                WIDTH {
					5-6: 2
					7-16: 4
				}
            }
        	LABEL {
                6-16 {
                	COLOR: @watercolor_lbl
                	OUTLINECOLOR: 255 255 255
                	FONT: 'scb'
                	TYPE: truetype
                	SIZE: 10
                    REPEATDISTANCE: 100
                    MINDISTANCE: 300
                	ANGLE: FOLLOW
                	POSITION: AUTO
                	PARTIALS: FALSE
                }
            } 
        }
    	CLASS {
            EXPRESSION: ([strokeweig] >= 0.350000001 AND [strokeweig] <= 0.6)
            STYLE {
                COLOR: @rivercolor
                WIDTH {
					5-6: 3
					7-16: 6
				}
            }
        	LABEL {
                6-16 {
                	COLOR: @watercolor_lbl
                	OUTLINECOLOR: 255 255 255
                	FONT: 'scb'
                	TYPE: truetype
                	SIZE: 10
                    REPEATDISTANCE: 100
                    MINDISTANCE: 300
                	ANGLE: FOLLOW
                	POSITION: AUTO
                	PARTIALS: FALSE
                }
            } 
        }
    	CLASS {
            EXPRESSION: ([strokeweig] >= 0.600000001 AND [strokeweig] <= 1)
            STYLE {
                COLOR: @rivercolor
                WIDTH {
					5-6: 4
					7-16: 8
				}
            }
        	LABEL {
                6-16 {
                	COLOR: @watercolor_lbl
                	OUTLINECOLOR: 255 255 255
                	FONT: 'scb'
                	TYPE: truetype
                	SIZE: 10
                    REPEATDISTANCE: 100
                    MINDISTANCE: 100
                	ANGLE: FOLLOW
                	POSITION: AUTO
                	PARTIALS: FALSE
                }
            } 
        }
    	CLASS {
            EXPRESSION: ([strokeweig] >= 1.000000001 AND [strokeweig] <= 2)
            STYLE {
                COLOR: @rivercolor
                WIDTH {
					5-6: 5
					7-16: 10
				}
            }
        	LABEL {
                6-16 {
                	COLOR: @watercolor_lbl
                	OUTLINECOLOR: 255 255 255
                	FONT: 'scb'
                	TYPE: truetype
                	SIZE: 10
                    REPEATDISTANCE: 100
                    MINDISTANCE: 300
                	ANGLE: FOLLOW
                	POSITION: AUTO
                	PARTIALS: FALSE
                }
            } 
        }
    }
}




